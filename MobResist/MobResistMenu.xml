						<Anchor point="TOP" relativeTo="MobResistDisplay">
							<Offset>
								<AbsDimension x="0" y="-10" />
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="TargetHolyText" inherits="GameFontNormalSmall" text="">
